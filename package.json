{
    "name": "test",
    "packageManager": "yarn@3.6.0",
    "private": true,
    "scripts": {
        "dev:client": "yarn workspace @reptalie-region-next/web run dev",
        "build:client": "yarn workspace @reptalie-region-next/web run build",
        "start:client": "yarn workspace @reptalie-region-next/web run start",
        "lint:client": "yarn workspace @reptalie-region-next/web run lint",
        "dev:server": "yarn workspace @reptalie-region-nest/server run start:dev",
        "build:server": "yarn workspace @reptalie-region-nest/server run build",
        "start:server": "yarn workspace @reptalie-region-nest/server run start",
        "lint:server": "yarn workspace @reptalie-region-nest/server run lint",
        "prod:server": "yarn workspace @reptalie-region-nest/server run start:prod",
        "debug:server": "yarn workspace @reptalie-region-nest/server run start:debug",
        "test:server": "yarn workspace @reptalie-region-nest/server run test",
        "g:typecheck": "yarn workspaces foreach -pv run typecheck",
        "lint-front": "lint-staged",
        "prepare": "husky install",
        "build:webview-bridge": "yarn workspace @reptalieregion/webview-bridge run build",
        "publish:webview-bridge": "yarn workspace @reptalieregion/webview-bridge run publish",
        "init:local:mongo": "docker-compose -f docker-compose-local.yml up -d",
        "init:local:network": "docker network create reptalie-region",
        "server:deploy:build": "docker-compose -f docker-compose-dev.yml up -d"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "@next/eslint-plugin-next": "^13.4.2",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "eslint-config-next": "^13.4.2"
    }
}
