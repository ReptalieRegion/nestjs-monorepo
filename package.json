{
    "name": "test",
    "packageManager": "yarn@4.1.0",
    "private": true,
    "scripts": {
        "server": "yarn workspace @crawl-nest/server",
        "build": "nest build",
        "build:server": "nest build apps/crawl-nest --debug --watch",
        "start:server": "nest start apps/crawl-nest",
        "dev:server": "nest start apps/crawl-nest --watch",
        "debug:server": "nest start apps/crawl-nest --debug --watch",
        "prod:server": "node dist/main",
        "g:typecheck": "yarn workspaces foreach -pv run typecheck",
        "lint-front": "lint-staged",
        "prepare": "husky install",
        "init:local:mongo": "docker-compose -f docker-compose-local.yml up -d",
        "init:local:network": "docker network create crawl",
        "server:deploy:build": "docker-compose -f docker-compose-dev.yml up -d"
    },
    "workspaces": [
        "apps/*",
        "packages/*",
        "private-packages/**"
    ],
    "dependencies": {
        "@aws-sdk/client-s3": "^3.344.0",
        "@nestjs/axios": "^3.0.1",
        "@nestjs/cache-manager": "^2.2.1",
        "@nestjs/common": "^10.3.3",
        "@nestjs/config": "^3.2.0",
        "@nestjs/core": "^10.3.3",
        "@nestjs/jwt": "^10.0.3",
        "@nestjs/mongoose": "^10.0.4",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.3",
        "@private-crawl/models": "workspace:^",
        "@private-crawl/types": "workspace:^",
        "@types/uuid": "^9.0.1",
        "axios": "^1.6.1",
        "bson": "^5.3.0",
        "cache-manager": "^5.2.1",
        "cache-manager-redis-yet": "^4.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dayjs": "^1.11.10",
        "eslint-config-next": "^13.4.2",
        "firebase-admin": "^11.11.0",
        "google-auth-library": "^9.2.0",
        "jsonwebtoken": "^9.0.0",
        "mime-types": "^2.1.35",
        "mongoose": "7.2.4",
        "multer": "^1.4.5-lts.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "redis": "^4.6.6",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.2.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "typescript": "5.0.4"
    }
}
